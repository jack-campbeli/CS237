    "# Your solution here\n",
    "def estimate_expected_orders(num_trials=10000):\n",
    "    orders = 0\n",
    "    for i in range(num_trials):\n",
    "        time = 0\n",
    "        while (time < 15):\n",
    "            time += random.uniform(0,15)\n",
    "            orders += 1\n",
    "    return orders/10000\n",
